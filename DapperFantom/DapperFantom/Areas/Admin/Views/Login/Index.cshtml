@model Admin


<div class="container">
    <div class="row">
        <form method="post">
            <input type="text" asp-for="@Model.Username" required />
            <hr />

            <input type="password" asp-for="@Model.Password" required />
            <hr />

            <button type="submit" class="btn btn-success">Login</button>
        </form>
    </div>
</div>